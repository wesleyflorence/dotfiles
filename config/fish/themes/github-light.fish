# GitHub Light Colors (from projekt0n/github-nvim-theme)
set -U fish_color_normal 24292f        # fg.default
set -U fish_color_command 0550ae       # prettylights constant (blue)
set -U fish_color_keyword cf222e       # prettylights keyword (red)
set -U fish_color_quote 0a3069         # prettylights string (dark blue)
set -U fish_color_error cf222e         # danger.fg
set -U fish_color_param 953800         # prettylights variable (orange)
set -U fish_color_comment 6e7781       # prettylights comment
set -U fish_color_autosuggestion 8c959f # fg.subtle
set -U fish_color_cwd 116329           # ansi green
set -U fish_pager_color_prefix 0550ae  # blue
set -U fish_pager_color_completion 57606a # fg.muted
set -U fish_pager_color_description 9a6700 # ansi yellow

# Hydro prompt colors
set -U hydro_color_pwd 24292f          # fg.default
set -U hydro_color_git 8250df          # purple
