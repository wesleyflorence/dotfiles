# GitHub Dark Colors (from projekt0n/github-nvim-theme)
set -U fish_color_normal e6edf3        # fg.default
set -U fish_color_command 79c0ff       # prettylights constant (bright blue)
set -U fish_color_keyword ff7b72       # prettylights keyword (red)
set -U fish_color_quote a5d6ff         # prettylights string (light blue)
set -U fish_color_error f85149         # danger.fg
set -U fish_color_param ffa657         # prettylights variable (orange)
set -U fish_color_comment 8b949e       # prettylights comment
set -U fish_color_autosuggestion 6e7681 # fg.subtle
set -U fish_color_cwd 3fb950           # ansi green
set -U fish_pager_color_prefix 79c0ff  # bright blue
set -U fish_pager_color_completion 848d97 # fg.muted
set -U fish_pager_color_description d29922 # ansi yellow

# Hydro prompt colors
set -U hydro_color_pwd e6edf3          # white (fg.default)
set -U hydro_color_git d2a8ff          # light purple
